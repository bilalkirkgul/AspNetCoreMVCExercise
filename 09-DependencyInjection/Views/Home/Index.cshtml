
@{
    ViewData["Title"] = "Index";
}

@model List<Product>

<div class="card">
    <div class="card-body">
        <table class="table table-hover">
            <thead>
                <tr>
                    <th>ProductID</th>
                    <th>ProductName</th>
                    <th>Stock</th>
                    <th>Price</th>
                </tr>
            </thead>
            <partial name="_productPartial"/>
        </table>
    </div>
</div>

